DROP DATABASE if exists BIRTHDAY;
CREATE DATABASE BIRTHDAY;
USE BIRTHDAY;

CREATE TABLE People(
id INTEGER PRIMARY KEY,
name VARCHAR(50) NOT NULL,
dob DATE NOT NULL,
age INTEGER,
imgURL VARCHAR(100)
)

